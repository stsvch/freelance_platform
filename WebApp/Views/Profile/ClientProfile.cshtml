@model ClientProfileModel
<div class="container">
    <h2>Профиль заказчика</h2>
    <p><strong>Название компании:</strong> @Model.CompanyName</p>
    <p><strong>Описание:</strong> @Model.Description</p>

    <h3>Созданные проекты</h3>
    <ul>
        @foreach (var project in Model.CreatedProjects)
        {
            <li><strong>@project.Title</strong> - Бюджет: @project.Budget</li>
        }
    </ul>
</div>


